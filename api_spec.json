{
    "openapi": "3.0.2",
    "info": {
        "title": "Travel APP API Spec",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://localhost:3000/api/v1"
        }
    ],
    "paths": {
        "/account": {
            "get": {
                "security": [
                    {
                        "ApiAuth": []
                    }
                ],
                "description": "List of operator accounts",
                "tags": [
                    "Admin API"
                ],
                "summary": "List of operator accounts",
                "responses": {
                    "200": {
                        "description": "Success get all operator account",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "string"
                                            },
                                            "name": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "security": [
                    {
                        "ApiAuth": []
                    }
                ],
                "description": "Create new operator account",
                "tags": [
                    "Admin API"
                ],
                "summary": "Create new opperator account",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "username": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Success create new account",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "code": {
                                            "type": "number"
                                        },
                                        "status": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "message": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/account/{accountId}": {
            "patch": {
                "security": [
                    {
                        "ApiAuth": []
                    }
                ],
                "description": "Reset Password",
                "tags": [
                    "Admin API"
                ],
                "summary": "Reset Password",
                "parameters": [
                    {
                        "schema": {
                            "type": "string"
                        },
                        "name": "accountId",
                        "in": "path",
                        "description": "Account Id",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success reset password",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/response"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Account not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/response"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "security": [
                    {
                        "ApiAuth": []
                    }
                ],
                "description": "Delete operator account by id",
                "tags": [
                    "Admin API"
                ],
                "summary": "Delete opreator account by id",
                "parameters": [
                    {
                        "schema": {
                            "type": "string"
                        },
                        "name": "accountId",
                        "in": "path",
                        "description": "Account Id",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success delete operator account",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/response"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Account not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/response"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/destinations": {
            "get": {
                "description": "List of destination",
                "tags": [
                    "User API",
                    "Operator API"
                ],
                "summary": "List of destination",
                "responses": {
                    "200": {
                        "description": "Success get all destination",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "code": {
                                            "type": "number"
                                        },
                                        "status": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "string"
                                                    },
                                                    "name": {
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "security": [
                    {
                        "ApiAuth": []
                    }
                ],
                "description": "Create new destination",
                "tags": [
                    "Operator API"
                ],
                "summary": "Create new destination",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/destination"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Success create new destination",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/response"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/destinations/{destinationId}": {
            "get": {
                "description": "Get destination by id",
                "tags": [
                    "User API"
                ],
                "summary": "Get destination by id",
                "parameters": [
                    {
                        "schema": {
                            "type": "string"
                        },
                        "name": "destinationId",
                        "in": "path",
                        "description": "Destination Id",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success get destination by id",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/destination"
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "security": [
                    {
                        "ApiAuth": []
                    }
                ],
                "description": "Update destination",
                "tags": [
                    "Operator API"
                ],
                "summary": "Update destination",
                "parameters": [
                    {
                        "schema": {
                            "type": "string"
                        },
                        "name": "destinationId",
                        "in": "path",
                        "description": "Destination Id",
                        "required": true
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/destination"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success Change destination",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/response"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "security": [
                    {
                        "ApiAuth": []
                    }
                ],
                "description": "Delete destination by id",
                "tags": [
                    "Operator API"
                ],
                "summary": "Delete destination by id",
                "parameters": [
                    {
                        "schema": {
                            "type": "string"
                        },
                        "name": "destinationId",
                        "in": "path",
                        "description": "Destination Id",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success delete destination by id",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/response"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/regions": {
            "get": {
                "description": "List of region",
                "tags": [
                    "User API"
                ],
                "summary": "List of region",
                "responses": {
                    "200": {
                        "description": "Success get all region",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "code": {
                                            "type": "number"
                                        },
                                        "status": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "string"
                                                    },
                                                    "name": {
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "ApiAuth": {
                "type": "apiKey",
                "in": "header",
                "name": "TOKEN-ROLE-Key",
                "description": "Authentication for API"
            }
        },
        "schemas": {
            "response": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "number"
                    },
                    "status": {
                        "type": "string"
                    },
                    "data": {
                        "type": "object",
                        "properties": {
                            "message": {
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "destination": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "date": {
                        "type": "string"
                    },
                    "image_file": {
                        "type": "object",
                        "properties": {
                            "title": {
                                "type": "string"
                            },
                            "mimetype": {
                                "type": "string"
                            },
                            "size": {
                                "type": "number"
                            }
                        }
                    },
                    "location": {
                        "type": "object",
                        "properties": {
                            "long": {
                                "type": "string"
                            },
                            "lat": {
                                "type": "string"
                            }
                        }
                    },
                    "text": {
                        "type": "string"
                    },
                    "rate": {
                        "type": "number"
                    },
                    "facilities": {
                        "type": "array",
                        "items": {
                            "type": "object"
                        }
                    },
                    "commanets": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "username": {
                                    "type": "string"
                                },
                                "message": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}